        <script>
            let scroller = 1

            function Scroller(event) {
                event.preventDefault()
                var water = document.getElementById("water")
                console.log("beast" + document.documentElement.scrollTop)
                if (document.body.scrollTop > scroller || document.documentElement.scrollTop > scroller) {
                    scroller = document.documentElement.scrollTop || document.body.scrollTop

                    if (document.documentElement.scrollTop > 50.0 && document.documentElement.scrollTop < 1000.0) {
                        water.style.transition = "0.8s all"
                        water.style['clip-path'] = 'polygon(51% 26%, 58% 66%, 60% 78%, 60% 88%, 56% 93%, 50% 95%, 44% 93%, 41% 88%, 41% 78%, 43% 67%)'

                        water.style.position = "fixed"
                        var prods = Array.from(document.querySelectorAll(".prod-img")).forEach(function(el) {

                            el.style.animation = "pause anime 3.3s ease-in-out alternate forwards"
                        })


                    } else {
                        water.style.position = "fixed"
                        water.style['clip-path'] = "polygon(50% 0%, 78% 0, 100% 0, 100% 10%, 77% 10%, 53% 10%, 37% 10%, 0 10%, 0 0, 16% 0)"
                        water.style['background-blend-mode'] = "darken"
                    }

                } else if (document.body.scrollTop < scroller || document.documentElement.scrollTop < scroller) {
                    scroller = document.body.scrollTop || document.documentElement.scrollTop
                    if (document.documentElement.scrollTop < 10.0) {
                        water.style['clip-path'] = 'polygon(50% 0%, 100% 0, 100% 35%, 100% 63%, 100% 100%, 59% 100%, 28% 100%, 0 100%, 0 40%, 0 0)'

                        water.style.position = "fixed"
                        var prods = Array.from(document.querySelectorAll(".prod-img")).forEach(function(el) {
                            el.style.animation = "anime 3.3s ease-in-out alternate forwards"
                        })

                    }
                }
                console.log(document.body.scrollTop)
            }
            if ('scrollRestoration' in history) { //FOR PREVENTING AUTOSCROLLING IF OCCUR

                history.scrollRestoration = 'manual';
            }

